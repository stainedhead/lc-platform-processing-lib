{
  "name": "@stainedhead/lc-platform-processing-lib",
  "version": "0.2.0",
  "description": "Processing logic library for Low Code Dev Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build:tsc",
    "build:tsc": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build",
    "test": "bun test",
    "test:domain": "bun test tests/domain",
    "test:use-cases": "bun test tests/use-cases",
    "test:adapters": "bun test tests/adapters",
    "test:contract": "bun test tests/contract",
    "test:integration": "bun test tests/integration",
    "test:watch": "bun test --watch",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "low-code",
    "processing",
    "library",
    "dev-platform"
  ],
  "author": "stainedhead",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stainedhead/lc-platform-processing-lib.git"
  },
  "bugs": {
    "url": "https://github.com/stainedhead/lc-platform-processing-lib/issues"
  },
  "homepage": "https://github.com/stainedhead/lc-platform-processing-lib#readme",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {}
}
